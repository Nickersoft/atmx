{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "preview": {
      "outputs": ["dist/**", ".vercel/**", ".astro/**"]
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".vercel/**", ".astro/**"]
    },
    "test": {
      "dependsOn": ["^test"]
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "dev": {
      "persistent": true,
      "cache": false
    }
  }
}