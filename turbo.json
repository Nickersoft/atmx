{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "preview": {
      "outputs": ["dist/**", ".vercel/**", ".astro/**"]
    },
    "build": {
      "dependsOn": ["^build", "@atmx-org/registry#check"],
      "outputs": ["dist/**", ".vercel/**", ".astro/**"]
    },
    "test": {
      "dependsOn": ["^test", "@atmx-org/registry#check"]
    },
    "check": {
      "outputs": [".astro/**"]
    },
    "dev": {
      "dependsOn": ["@atmx-org/registry#check"],
      "persistent": true,
      "cache": false
    }
  }
}