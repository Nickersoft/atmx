---
import type { Snippet } from "@atmx-org/common";

export const prerender = false;

const snippets = await fetch(new URL("/registry/helpers.json", Astro.url))
  .then((res) => res.json())
  .then((res) => Object.values(res) as Snippet[]);

return Astro.redirect(`/${Astro.params.type}/${snippets[0]?.name}`, 302);
